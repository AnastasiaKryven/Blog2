
@{
    ViewBag.Title = "About";
}

@model IEnumerable<Blog.Domain.Entities.Request>

<h1>Отзывы</h1>
@foreach (var it in ViewBag.Requests)
{
    <div class="sticker-right sticker-right align-text-top" data-sticker="@it.DateRequest"><i>@it.Name</i><br />@it.TextOfRequest</div>
}

